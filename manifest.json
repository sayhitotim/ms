{
  "name": "Metasearch",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Measure metasearch timings.",
  "icons": { "128": "icon.png" },
  "content_scripts": [
    {
      "matches": ["http://www.roomguru.ru/Hotels/Search*"],
      "css": ["style.css"],
      "js": ["jquery.min.js", "underscore.js", "moment.min.js", "hc_serp.js"]
    }
  ],
  "permissions": [
    "tabs", "http://www.roomguru.ru/Hotels/Search*"
  ]
}